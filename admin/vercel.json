{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "dist/admin/src/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/admin/src/server.js" }
  ],
  "env": {
    "PORT": "80",
    "PAYLOAD_CONFIG_PATH": "dist/admin/src/configs/payload.config.js"
  }
}
