{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "npm run build:admin:express",
  "outputDirectory": "apps/admin-express",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "apps/admin-express/build/**"
    }
  },
  "env": {
    "NODE_ENV": "production",
    "PAYLOAD_CONFIG_PATH": "dist/configs/payload.config.js"
  }
}
