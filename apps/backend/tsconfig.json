{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@apps/api-gateway": [
        "apps/api-gateway/src"
      ],
      "@apps/api-gateway/*": [
        "apps/api-gateway/src/*"
      ],
      "@apps/auth": [
        "apps/auth/src"
      ],
      "@apps/auth/*": [
        "apps/auth/src/*"
      ],
      "@apps/identity": [
        "apps/identity/src"
      ],
      "@apps/identity/*": [
        "apps/identity/src/*"
      ],
      "@libs/common": [
        "libs/common/src"
      ],
      "@libs/common/*": [
        "libs/common/src/*"
      ],
      "@libs/grpc": [
        "libs/grpc/src"
      ],
      "@libs/grpc/*": [
        "libs/grpc/src/*"
      ],
      "@proto/*": [
        "libs/grpc/src/generated/*"
      ],
      "@libs/exceptions": [
        "libs/exceptions/src"
      ],
      "@libs/exceptions/*": [
        "libs/exceptions/src/*"
      ]
    }
  }
}