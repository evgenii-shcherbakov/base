{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "framework": null,
  "builds": [
    {
      "src": "apps/backend/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/backend/dist/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
