{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "framework": "angular",
  "installCommand": "npm install",
  "buildCommand": "npm run build:frontend",
  "outputDirectory": "apps/frontend/dist/frontend",
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "apps/frontend/dist/frontend/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
