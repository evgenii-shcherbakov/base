{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "framework": "angular",
  "builds": [
    {
      "src": "apps/frontend/dist/frontend/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "apps/frontend/dist/frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/frontend/dist/frontend/server/server.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
