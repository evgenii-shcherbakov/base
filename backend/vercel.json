{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [{ "src": "dist/backend/src/main.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/backend/src/main.js",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}
