{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "installCommand": "npm run deploy:install:backend",
  "buildCommand": "npm run deploy:build:backend",
  "outputDirectory": "backend/dist",
  "builds": [
    {
      "src": "backend/dist/backend/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/dist/backend/src/main.js",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}
