{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "installCommand": "npm run deploy:install:frontend",
  "buildCommand": "npm run deploy:build:frontend",
  "outputDirectory": "frontend/dist/frontend",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/api"
    }
  ],
  "functions": {
    "frontend/api/index.js": {
      "includeFiles": "frontend/dist/frontend/browser/**"
    }
  }
}
