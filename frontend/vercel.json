{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "outputDirectory": "dist/frontend",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}
