{
  "name": "evgenii-shcherbakov.github.io",
  "private": true,
  "author": "Evgenii Shcherbakov <iipekolict@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev",
    "dev:backend": "turbo run dev --filter=backend",
    "build": "turbo run build",
    "build:backend": "turbo run build --filter=backend",
    "format": "turbo run format",
    "gen:package": "turbo gen package && yarn",
    "reset": "rm -rf .turbo && turbo run reset",
    "reset:backend": "turbo run reset --filter=backend",
    "docker:local": "docker-compose -f docker-compose.yml -p platform --profile local up --build",
    "docker": "docker-compose -f docker-compose.yml -p platform --profile all up --build"
  },
  "devDependencies": {
    "@turbo/gen": "^1.13.2",
    "prettier": "^3.2.5",
    "turbo": "^1.13.2",
    "yarn": "^1.22.22"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
