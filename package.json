{
  "name": "evgenii-shcherbakov.github.io",
  "private": true,
  "author": "Evgenii Shcherbakov <iipekolict@gmail.com>",
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:admin": "turbo run dev --filter=admin",
    "dev:backend": "turbo run dev --filter=\"backend.*\"",
    "dev:backend.api-gateway": "turbo run dev --filter=backend.api-gateway",
    "dev:backend.identity": "turbo run dev --filter=backend.identity",
    "build": "turbo run build",
    "build:admin": "turbo run build --filter=admin",
    "build:backend": "turbo run build --filter=\"backend.*\"",
    "build:backend.api-gateway": "turbo run build --filter=backend.api-gateway",
    "build:backend.identity": "turbo run build --filter=backend.identity",
    "prod": "turbo run prod",
    "prod:admin": "turbo run prod --filter=admin",
    "prod:backend": "turbo run prod --filter=\"backend.*\"",
    "prod:backend.api-gateway": "turbo run prod --filter=backend.api-gateway",
    "prod:backend.identity": "turbo run prod --filter=backend.identity",
    "format": "turbo run format",
    "gen:package": "turbo gen package && yarn",
    "gen:backend-package": "turbo gen backend-package && yarn",
    "gen:backend-microservice": "turbo gen backend-microservice && yarn",
    "gen:backend-entity": "turbo gen backend-entity && turbo run build --filter=@packages/common",
    "reset": "turbo run reset && rm -rf .turbo node_modules yarn.lock",
    "reset:admin": "turbo run reset --filter=admin",
    "reset:backend": "turbo run reset --filter=\"backend.*\"",
    "docker:local": "docker-compose -f docker-compose.local.yml -p platform-local up --build",
    "docker": "docker-compose -f docker-compose.yml -p platform up --build"
  },
  "devDependencies": {
    "@turbo/gen": "^1.13.2",
    "change-case-all": "^2.1.0",
    "prettier": "^3.2.5",
    "turbo": "^1.13.2",
    "yarn": "^1.22.22"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
