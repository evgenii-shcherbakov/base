{
  "name": "@shared/cv",
  "private": true,
  "author": "Evgenii Shcherbakov <iipekolict@gmail.com>",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "format": "prettier src --write --config ../../.prettierrc",
    "dev": "nodemon",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig-cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run patch",
    "patch": "npm run --workspace=@shared/scripts patch-package auth",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "@shared/core": "*"
  },
  "devDependencies": {
    "@shared/scripts": "*",
    "@shared/ts-configs": "*",
    "@types/node": "^20.11.8",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "typescript": "latest"
  }
}
